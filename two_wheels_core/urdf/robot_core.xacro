<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find two_wheels_core)/urdf/materials.xacro"/>
    <!--    <xacro:include filename="$(find two_wheels_core)/urdf/two_wheels_core.trans"/>-->
    <!--    <xacro:include filename="$(find two_wheels_core)/urdf/two_wheels_core.gazebo"/>-->
    <link name="base_link">
        <inertial>
            <origin xyz="0.0 2.3684273442397533e-17 0.025" rpy="0 0 0"/>
            <mass value="6.028923307511652"/>
            <inertia ixx="0.02914" iyy="0.013063" izz="0.041801" ixy="0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find two_wheels_core)/urdf/meshes/base_link.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find two_wheels_core)/urdf/meshes/base_link.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="rear_left_wheel">
        <inertial>
            <origin xyz="-0.012500000000000011 -2.7755575615628914e-17 0.0" rpy="0 0 0"/>
            <mass value="0.3549714991722238"/>
            <inertia ixx="0.000102" iyy="7e-05" izz="7e-05" ixy="0.0" iyz="-0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.08 0.078 -0.025" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find two_wheels_core)/urdf/meshes/rear_left_wheel.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0.08 0.078 -0.025" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find two_wheels_core)/urdf/meshes/rear_left_wheel.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="rear_left_wheel_joint" type="continuous">
        <origin xyz="-0.08 -0.078 0.025" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="rear_left_wheel"/>
        <axis xyz="1.0 0.0 0.0"/>
    </joint>

    <link name="front_right_wheel">
        <inertial>
            <origin xyz="0.012499999999999997 2.7755575615628914e-17 0.0" rpy="0 0 0"/>
            <mass value="0.3549714991722238"/>
            <inertia ixx="0.000102" iyy="7e-05" izz="7e-05" ixy="0.0" iyz="0.0" ixz="-0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.08 -0.078 -0.025" rpy="0 0 0"/>
