    <link name="rear_right_wheel">
        <inertial>
            <origin xyz="0.012499999999999997 -2.7755575615628914e-17 0.0" rpy="0 0 0"/>
            <mass value="0.3549714991722238"/>
            <inertia ixx="0.000102" iyy="7e-05" izz="7e-05" ixy="0.0" iyz="-0.0" ixz="-0.0"/>
